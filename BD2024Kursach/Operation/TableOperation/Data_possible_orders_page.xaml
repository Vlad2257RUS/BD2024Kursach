<Page x:Class="BD2024Kursach.Operation.TableOperation.Data_possible_orders_page"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BD2024Kursach.Operation.TableOperation"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Data_possible_orders_page">

    <Grid Margin="5">

        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>


        </Grid.RowDefinitions>

        <Label Grid.Row="0" Content="Номер возможного заказа:"/>
        <TextBox x:Name="NumberPossible_TextBox" Grid.Row="1"/>

        <Label Grid.Row="2" Content="Канал:"/>
        <ComboBox x:Name="Channel_po_ComboBox" Grid.Row="3"/>

        <Label Grid.Row="4" Content="Тип времени:"/>
        <ComboBox x:Name="Type_time_po_ComboBox" Grid.Row="5"/>

    </Grid>
</Page>
